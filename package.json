{
  "name": "builder-victory-component",
  "version": "4.0.1",
  "description": "Builder Archetype - Victory Component",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/builder-victory-component.git"
  },
  "author": "Ryan Roemer <ryan.roemer@formidablelabs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/builder-victory-component/issues"
  },
  "homepage": "https://github.com/FormidableLabs/builder-victory-component",
  "config": {
    "wds_port_dev": 3000,
    "wds_port_test": 3001
  },
  "scripts": {
    "npm:postinstall": "cd lib || builder run build --expand-archetype",
    "npm:preversion": "builder run check",
    "npm:version": "builder run clean && builder run build",
    "npm:postversion": "publishr postversion -V",
    "npm:postpublish": "publishr postpublish -V",
    "npm:test": "builder run test-frontend",
    "version-dry-run": "publishr dry-run -V",
    "clean-dist": "rimraf dist",
    "clean-lib": "rimraf lib",
    "clean": "builder run clean-lib && builder run clean-dist",
    "build-dist-min": "webpack --bail --config node_modules/builder-victory-component/config/webpack/webpack.config.js --colors",
    "build-dist-dev": "webpack --bail --config node_modules/builder-victory-component/config/webpack/webpack.config.dev.js --colors",
    "build-dist": "builder run clean-dist && builder run build-dist-min && builder run build-dist-dev",
    "build-lib": "builder run clean-lib && babel src -d lib --copy-files",
    "build-watch": "babel src -d lib --copy-files -w",
    "build": "builder run build-lib && builder run build-dist",
    "server-dev": "webpack-dev-server --config node_modules/builder-victory-component/config/webpack/demo/webpack.config.dev.js --colors --content-base demo",
    "server-hot": "webpack-dev-server --config node_modules/builder-victory-component/config/webpack/demo/webpack.config.hot.js --colors --inline --hot --content-base demo",
    "server-test": "webpack-dev-server --config node_modules/builder-victory-component/config/webpack/webpack.config.test.js --colors",
    "dev": "builder concurrent server-dev server-test",
    "hot": "builder concurrent server-hot server-test",
    "open-demo": "opener http://127.0.0.1:3000",
    "open-dev": "builder concurrent dev open-demo",
    "open-hot": "builder concurrent hot open-demo",
    "lint-source": "eslint --color --ext .js,.jsx src",
    "lint-demo": "eslint --color --ext .js,.jsx demo",
    "lint-perf": "eslint --color --ext .js,.jsx perf",
    "lint-test": "eslint --color --ext .js,.jsx test",
    "lint": "builder concurrent lint-source lint-demo lint-test",
    "test-frontend": "karma start node_modules/builder-victory-component/config/karma/karma.conf.js",
    "test-frontend-ci": "karma start --browsers PhantomJS,Firefox node_modules/builder-victory-component/config/karma/karma.conf.coverage.js",
    "test-frontend-cov": "karma start node_modules/builder-victory-component/config/karma/karma.conf.coverage.js",
    "test-frontend-dev": "karma start node_modules/builder-victory-component/config/karma/karma.conf.dev.js",
    "test-frontend-perf": "karma start node_modules/builder-victory-component/config/karma/karma.conf.perf.js",
    "test-ci": "builder run test-frontend-ci",
    "test-cov": "builder run test-frontend-cov",
    "test-dev": "builder run test-frontend-dev",
    "test-perf": "builder run test-frontend-perf",
    "check": "builder run lint && builder run npm:test",
    "check-ci": "builder run lint && builder run test-ci",
    "check-cov": "builder run lint && builder run test-cov",
    "check-dev": "builder run lint && builder run test-dev",
    "check-perf": "builder run lint-perf && builder run test-perf",
    "builder:lint-config": "eslint --no-eslintrc --config config/eslint/.eslintrc-config config",
    "builder:lint": "npm run builder:lint-config",
    "builder:check": "npm run builder:lint"
  },
  "dependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^3.1.2",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-native": "^1.9.0",
    "lodash-webpack-plugin": "^0.7.0",
    "rimraf": "^2.4.0",
    "webpack": "^1.12.9"
  },
  "devDependencies": {
    "builder-support": "^0.4.0",
    "eslint": "^2.0.0",
    "eslint-config-formidable": "^2.0.1",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.2.0"
  }
}
